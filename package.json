  "scripts": {
    "dev": "nx run-many --target=dev --all --parallel",
    "migration:generate": "ts-node --project apps/api/tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d apps/api/src/data-source.ts apps/api/src/database/migrations/$npm_config_name",
    "migration:create": "ts-node --project apps/api/tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create apps/api/src/database/migrations/$npm_config_name",
    "migration:run": "ts-node --project apps/api/tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d apps/api/src/data-source.ts",
    "migration:revert": "ts-node --project apps/api/tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d apps/api/src/data-source.ts"
  },
